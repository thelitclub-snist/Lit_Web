{
  "version": 2,
  "buildCommand": "pnpm run build",
  "outputDirectory": "dist/public",
  "routes": [
    {
      "src": "^/api/trpc/(.*)$",
      "dest": "/api/trpc/[...all].ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/trpc/[...all].ts": {
      "maxDuration": 30
    }
  }
}
